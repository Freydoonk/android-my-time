<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="fill_parent" android:layout_width="fill_parent"
	android:orientation="horizontal" android:padding="10sp">
	<TextView android:id="@+id/start_date_time"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:text="10-02-25 10:13" />
	<TextView android:id="@+id/spacing" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_toRightOf="@+id/start_date_time"
		android:text=" - " />
	<TextView android:id="@+id/end_date_time"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:layout_toRightOf="@+id/spacing" android:text="10-02-25 12:20" />
	<TextView android:layout_toRightOf="@+id/end_date_time"
		android:layout_width="fill_parent" android:id="@+id/hours"
		android:layout_height="wrap_content" android:gravity="right" android:text="3.5h" />
	<TextView android:id="@+id/comment" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_below="@+id/start_date_time"
		android:text="changed idl" />

</RelativeLayout>
